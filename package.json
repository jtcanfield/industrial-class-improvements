    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "hbs": "^4.0.3",
    "mailgun-js": "^0.22.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "paypal-rest-sdk": "^1.8.1",
    "react": "^16.8.4",
    "react-cookies": "^0.1.0",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.2.2",
