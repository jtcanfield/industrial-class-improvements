{
  "name": "industrial-class-improvements",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "npm run client:build && concurrently \"npm run server:watch\" \"npm run client:watch\" --raw",
    "server:watch": "nodemon ./bin/www",
    "client:watch": "onchange \"src/**\" -- npm run client:build",
    "client:build": "webpack",
    "client:buildProd": "webpack",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx app.js ./src",
    "fix": "./node_modules/.bin/eslint --ext .js,.jsx app.js ./src --fix"
  },
  "dependencies": {
    "@axetroy/react-download": "^1.0.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.0",
    "express-validator": "^5.3.1",
    "hbs": "^4.0.1",
    "mailgun-js": "^0.22.0",
    "morgan": "^1.9.0",
    "react": "^16.1.1",
    "react-cookies": "^0.1.0",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "superagent": "^3.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^3.0.1",
    "nodemon": "^1.18.9",
    "onchange": "^5.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  }
}
