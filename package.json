{
  "name": "industrial-class-improvements",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "npm run client:build && concurrently \"npm run server:watch\" \"npm run client:watch\" --raw",
    "server:watch": "nodemon ./bin/www",
    "client:watch": "onchange \"src/**\" -- npm run client:build",
    "client:build": "webpack",
    "client:buildProd": "webpack",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx app.js ./src",
    "fix": "./node_modules/.bin/eslint --ext .js,.jsx app.js ./src --fix"
  },
